{
  "cacheBust": true,
  "cleanPublic": true,
  "defaultPattern": "pages-index",
  "defaultShowPatternInfo": false,
  "ishControlsHide": {
    "s": false,
    "m": false,
    "l": false,
    "full": true,
    "random": true,
    "disco": true,
    "hay": true,
    "mqs": false,
    "find": false,
    "views-all": true,
    "views-annotations": false,
    "views-code": false,
    "views-new": false,
    "tools-all": false,
    "tools-docs": true
  },
  "ishViewportRange": {
    "s": [
      320,
      500
    ],
    "m": [
      500,
      800,
      500,
      800
    ],
    "l": [
      800,
      2600,
      800,
      2600
    ]
  },
  "logLevel": "info",
  "outputFileSuffixes": {
    "rendered": ".rendered",
    "rawTemplate": "",
    "markupOnly": ".markup-only"
  },
  "paths": {
    "source": {
      "root": "source/",
      "patterns": "source/_patterns/",
      "data": "source/_data/",
      "meta": "source/_meta/",
      "annotations": "source/_annotations/",
