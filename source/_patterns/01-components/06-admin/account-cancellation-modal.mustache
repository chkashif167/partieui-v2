<div class="modal feedback-modal dynamic-height" id="account-cancellation-modal">
    <div class="modal-overlay"></div>
    <div class="modal-container">
        <div class="modal-header">
            <h2 class="modal-title">Subscription cancellation reason</h2>
            <button class="btn bg-black btn-close" onclick="closeModal()">
                <img src="/images/icons/misc/close-x.svg" alt="Close Icon">
            </button>
        </div>
        <div class="modal-content">
            <div class="form-control">
                <textarea name="feedback" id="feedback" placeholder="Type message"></textarea>
            </div>
            <button class="btn bg-teal submit" onclick="closeModal()">Cancel subscription</button>
        </div>
    </div>
</div>