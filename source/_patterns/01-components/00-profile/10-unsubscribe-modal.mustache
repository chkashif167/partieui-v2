<div class="modal leave-modal dynamic-height" id="unsubscribe-modal">
    <div class="modal-overlay"></div>
    <div class="modal-container">
        <div class="modal-header">
            <h2 class="modal-title"></h2>
            <button class="btn bg-black btn-close" onclick="closeModal()">
                <img src="/images/icons/misc/close-x.svg" alt="Close Icon">
            </button>
        </div>
        <div class="modal-content">
            <h2>Are you sure you want to unfollow?</h2>
            <div class="btn-group">
                <button class="btn btn-outline" onclick="closeModal()">Cancel</button>
                <button class="btn bg-teal" onclick="closeModal()">Yes</button>
            </div>
        </div>
    </div>
</div>
